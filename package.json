    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hook-form": "^6.0.8",
    "react-image-gallery": "^1.0.8",
    "sass": "^1.26.10",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.88.2",
    "swr": "^0.3.3",
    "unfetch": "^4.1.0",
    "yup": "^0.29.1"
